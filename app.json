{
  "name": "Monstera",
  "description": "Monstera",
  "repository": "https://github.com/sideroad/monstera",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "env": {
    "NODE_ENV": {
      "value": "production"
    },
    "NODE_PATH": {
      "value": "./src"
    },
    "NPM_CONFIG_PRODUCTION": {
      "value": "false"
    },
    "APIKIT_HOST": {
      "description": "Please specify domain"
    },
    "APIKIT_PORT": {
      "value": "80"
    }
  },
  "addons": [
    "mongolab:sandbox"
  ]
}
